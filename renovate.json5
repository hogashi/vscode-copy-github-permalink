{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
  ],
  dependencyDashboardApproval: true,
  labels: [
    'renovate',
  ],
  packageRules: [
    {
      matchUpdateTypes: [
        'minor',
        'patch',
        'pin',
        'digest',
      ],
      automerge: true,
      postUpdateOptions: [
        'yarnDedupeHighest',
      ],
    },
  ],
}
